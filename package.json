{
  "name": "docsql",
  "version": "0.4.0",
  "homepage": "https://github.com/peterbe/docsql",
  "license": "MIT",
  "scripts": {
    "analyze": "npm run run -- --analyze-only",
    "dev": "npm run analyze && next dev",
    "build": "next build",
    "clear": "rm -rf .next out",
    "start": "next start",
    "lint": "next lint && npm run prettier-check",
    "prettier-check": "prettier -c \"**/*.{ts,tsx,js,mjs,scss,yml,yaml}\"",
    "prettier-writer": "prettier -w \"**/*.{ts,tsx,js,mjs,scss,yml,yaml}\"",
    "export": "next export",
    "build:export": "npm run build && npm run export",
    "run": "node cli/index.mjs",
    "build:export:run": "npm run build:export && npm run run",
    "test": "echo 'Tested in Actions'",
    "prepare": "husky install"
  },
  "bin": "cli/index.mjs",
  "files": [
    "/out",
    "/lib",
    "/cli",
    "/builtin-plugins"
  ],
  "dependencies": {
    "cli-progress": "3.10.0",
    "commander": "9.0.0",
    "dotenv": "16.0.0",
    "fdir": "5.2.0",
    "gray-matter": "4.0.3",
    "open-editor": "4.0.0",
    "polka": "0.5.2",
    "sirv": "2.0.2"
  },
  "devDependencies": {
    "@mantine/core": "3.6.6",
    "@mantine/hooks": "3.6.6",
    "@mantine/next": "3.6.14",
    "@modulz/radix-icons": "4.0.0",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "@types/refractor": "3.0.2",
    "alasql": "1.7.3",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "husky": "7.0.4",
    "next": "12.1.0",
    "np": "7.6.0",
    "prettier": "2.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-iconly": "2.2.5",
    "react-refractor": "2.1.6",
    "swr": "1.2.2",
    "typescript": "4.6.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/peterbe/docsql.git"
  },
  "bugs": {
    "url": "https://github.com/peterbe/docsql/issues"
  }
}
