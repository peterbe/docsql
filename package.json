{
  "name": "docsql",
  "version": "0.11.7",
  "homepage": "https://github.com/peterbe/docsql",
  "license": "MIT",
  "scripts": {
    "analyze": "npm run run -- --analyze-only",
    "dev": "npm run analyze && next dev",
    "build": "next build",
    "clear": "rm -rf .next out",
    "start": "next start",
    "lint": "next lint && npm run prettier-check",
    "prettier-check": "prettier -c \"**/*.{ts,tsx,js,mjs,scss,yml,yaml}\"",
    "prettier-writer": "prettier -w \"**/*.{ts,tsx,js,mjs,scss,yml,yaml}\"",
    "export": "next export",
    "build:export": "npm run build && npm run export",
    "run": "node cli/index.mjs",
    "build:export:run": "npm run build:export && npm run run",
    "test": "echo 'Tested in Actions'",
    "prepare": "husky install",
    "tsc": "tsc --noEmit",
    "release": "npm run build:export && np"
  },
  "bin": "cli/index.mjs",
  "files": [
    "/out",
    "/lib",
    "/cli",
    "/builtin-plugins"
  ],
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/server": "11.10.0",
    "@tabler/icons-react": "2.13.1",
    "cli-progress": "3.12.0",
    "commander": "10.0.0",
    "dotenv": "16.0.3",
    "fdir": "6.0.1",
    "gray-matter": "4.0.3",
    "open-editor": "4.0.0",
    "polka": "0.5.2",
    "sirv": "2.0.2"
  },
  "devDependencies": {
    "@mantine/core": "5.10.5",
    "@mantine/hooks": "5.10.5",
    "@mantine/next": "5.10.5",
    "@types/node": "18.14.2",
    "@types/react": "18.0.32",
    "@types/refractor": "3.0.2",
    "alasql": "3.1.0",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.3",
    "husky": "8.0.3",
    "next": "13.2.3",
    "np": "7.6.3",
    "prettier": "2.8.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-refractor": "2.1.7",
    "swr": "2.0.4",
    "typescript": "5.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/peterbe/docsql.git"
  },
  "bugs": {
    "url": "https://github.com/peterbe/docsql/issues"
  }
}
